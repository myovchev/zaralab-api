{
  "name": "zaralab/api",
  "description": "Slim3 based ZaraLab API",
  "license": "BSD-3-Clause",
  "type": "project",
  "keywords": [
    "zaralab",
    "slim-framework"
  ],
  "authors": [
    {
      "name": "Miroslav Yovchev",
      "email": "m.yovchev@corllete.com",
      "homepage": "http://corllete.com"
    },
    {
      "name": "George Petkov",
      "email": "gpetkoff98@abv.bg"
    },
    {
      "name": "Radoslav Enev",
      "email": "rado_bg98@abv.bg"
    },
    {
      "name": "Viktor Terziev"
    }
